            0% {
                transform: translate(-100px, 200px);
            }

            50% {
                transform: translate(-90px, 250px);
            }

            100% {
                transform: translate(-100px, 200px);
            }
        }

        @-webkit-keyframes rotating_window {
            0% {
                transform: rotate(0);
            }

            50% {
                transform: rotate(5deg);
            }

            100% {
                transform: rotate(0);
            }
        }

