{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    //React options
    "jsx": "react-jsx",
    "jsxImportSource": "react",

    //Aliases
    "paths": {
      "@components/*": ["./src/components"],
      "@layouts/*": ["./src/layouts"],
      "@pages/*": ["./src/pages"],
    },
  }
}